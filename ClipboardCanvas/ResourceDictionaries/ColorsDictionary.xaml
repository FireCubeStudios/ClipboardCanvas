<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <Color x:Key="SolidBackgroundAcrylic">#FFFFFF</Color>
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush">#FFFFFF</SolidColorBrush>
            <SolidColorBrush x:Key="SubtleBackgroundBrush">#B3FFFFFF</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="SolidBackgroundAcrylic">#2E2E2E</Color>
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush">#201F1E</SolidColorBrush>
            <SolidColorBrush x:Key="SubtleBackgroundBrush">#0FFFFFFF</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
